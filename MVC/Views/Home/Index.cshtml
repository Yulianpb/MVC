@{
    ViewData["Title"] = "Home Page";
    string mensaje = "Este es un mensaje desde una variable";
    int edad = 24;
    decimal dec = 10m;
    bool isTrue = false;

    List<string> nombres = new() { "Delmy", "Pedro", "Adonys" };
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <p>@mensaje</p>
    <p>@edad</p>
    <p>@dec</p>
    <p>@isTrue</p>
    @{
        for (int i = 0; i < nombres.Count(); i++)
        {
            <p>@nombres[i]</p>
        }
    }
    <p>@ViewBag.Num1</p>
    <p>@ViewBag.Num2</p>
    <p>@(ViewBag.Num1 + @ViewBag.Num2)</p)

</div>
