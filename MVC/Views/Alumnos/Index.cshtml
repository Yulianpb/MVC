@model List<Alumnos>;
<div class="container">
	<div class="row">
		<div class="col-xl-12 col-md-12 col-sm-12">
			<table class="table table-hover">
				<thead>
					<th>Id</th>
					<th>Nombres</th>
					<th>Apellidos</th>
				</thead>
				<tbody>
					@foreach (Alumnos alumnos in Model)
					{
						<tr>
							<td>@alumnos.AlumnoId</td>
							<td>@alumnos.Nombres</td>
							<td>@alumnos.Apellidos</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>
</div>